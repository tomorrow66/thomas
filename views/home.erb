		<div id="biography">		  
		 <%= biography(@bio.title, @bio.article) %>	
		</div>
		

		<div id="video" name="video">
			<h2>VIDEO</h2>
			<div id="accordion">
				<% @videos = @videos.reverse %>
				<% if @videos == nil then %>
				  <p>Check back soon for videos</p>
				<% else %>
					<% @videos.each do |v| %>
						<h3>
							<a href="<%= v.link %>" rel="prettyPhoto[slide]" class="upcase">&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; <%= v.title.upcase! %><h2><%= v.created_at.display %></h2></a>
						</h3>

					<% end %>
				<% end %>
			</div>
		</div>

		<a href="#top" class="top">Back To Top</a>

		<div id="news" name="news">
			<h2>NEWS</h2>
			
			<div id="accordion">
			  <% @posts = @posts.reverse %>
			  <% @posts.each do |post| %>
			  <%= entry(post.title, post.created_at.display, post.article) %>
			  <% end %>
			</div>
		</div>
		<a href="#top" class="top">Back To Top</a>
	</div>

